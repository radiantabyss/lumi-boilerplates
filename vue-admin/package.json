{
    "name": "lumi-vue-boilerplate",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "serve": "php artisan sass && php artisan static && vue-cli-service serve",
        "watch": "vue-cli-service serve",
        "build": "vue-cli-service build",
        "build-staging": "vue-cli-service build --mode staging",
        "build-master": "vue-cli-service build --mode master",
        "lint": "vue-cli-service lint",
        "sprite": "echo \"no more png sprites\"",
        "sass": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --config=webpack.sass.config.js --mode development"
    },
    "dependencies": {
        "@radiantabyss/lumi-vue": "^1.2.7",
        "axios": "0.18.0",
        "core-js": "2.6.5",
        "qs": "6.7.0",
        "vue": "2.*",
        "vue-js-modal": "^1.3.35",
        "vue-meta": "1.6.0",
        "vue-router": "3.0.6",
        "vuex": "3.1.0",
        "@ivanv/vue-collapse-transition": "0.2.1",
        "vue-clipboard2": "0.3.3",
        "dropzone": "5.5.1",
        "moment": "2.24.0",
        "moment-timezone": "0.5.43",
        "vue-select": "3.18.3",
        "vue-slider-component": "3.2.15",
        "vue2-datepicker": "3.11.1"
    },
    "devDependencies": {
        "@vue/cli-plugin-babel": "3.7.0",
        "@vue/cli-plugin-eslint": "3.7.0",
        "@vue/cli-service": "3.7.0",
        "babel-eslint": "10.0.1",
        "cross-env": "5.2.0",
        "disable-output-webpack-plugin": "1.0.1",
        "eslint": "5.16.0",
        "eslint-plugin-vue": "5.0.0",
        "extract-text-webpack-plugin": "4.0.0-beta.0",
        "mini-css-extract-plugin": "0.9.0",
        "node-sass": "4.12.0",
        "sass-loader": "7.1.0",
        "style-loader": "0.23.1",
        "svg-sprite-generator": "0.0.7",
        "vue-template-compiler": "2.5.21",
        "webpack-spritesmith": "1.0.1"
    },
    "eslintConfig": {
        "root": true,
        "env": {
            "node": true
        },
        "extends": [
            "plugin:vue/essential",
            "eslint:recommended"
        ],
        "rules": [],
        "parserOptions": {
            "parser": "babel-eslint"
        },
        "globals": {
            "Alert": "writable",
            "Auth": "writable",
            "Cookie": "writable",
            "Domain": "writable",
            "Gate": "writable",
            "Item": "writable",
            "Items": "writable",
            "Mixins": "writable",
            "ReactiveStorage": "writable",
            "Request": "writable",
            "Settings": "writable",
            "StorageHandler": "writable",
            "Str": "writable",
            "app_url": "writable",
            "back_url": "writable",
            "uploads_url": "writable",
            "is_dev": "writable",
            "is_local": "writable",
            "is_production": "writable"
        }
    },
    "postcss": {
        "plugins": {
            "autoprefixer": []
        }
    },
    "browserslist": [
        "> 1%",
        "last 2 versions"
    ]
}